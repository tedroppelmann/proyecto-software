<h1>Nueva comuna</h1>

<p><%= notice %></p>

<%= form_with(model: @comuna, method: :post) do |form|%>
    <div class="field">
        <%=form.label :nombre%>
        <%=form.text_field :nombre%>
    </div>

    <div class="action">
        <%=form.submit%>
    </div>

<% end %>

<%= link_to "ver listado de comunas", comunas_index_path%>